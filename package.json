{
  "name": "js-automation-examples",
  "version": "0.1.0",
  "description": "Some examples of JS-powered automation",
  "main": "index.js",
  "scripts": {
    "sass": "node-sass src/scss/main.scss dist/css/compiled.css",
    "prefix": "autoprefixer dist/css/compiled.css --browsers \"last 2 version\" --no-map -o dist/css/compiled-prefixed.css",
    "cssmin": "cleancss dist/css/compiled-prefixed.css -o dist/css/compiled-prefixed-minified.css",
    "clean:css": "rimraf dist/css/*.*",
    "prestyle:io": "npm run clean:css",
    "style:io": "npm run sass && npm run prefix && npm run cssmin",
    "prestyle:stream": "npm run clean:css",
    "style:stream": "node-sass src/scss/main.scss | autoprefixer --browsers \"last 2 version\" -o - | cleancss > dist/css/compiled-prefixed-minified.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidtheclark/js-automation-examples.git"
  },
  "author": "David Clark",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davidtheclark/js-automation-examples/issues"
  },
  "homepage": "https://github.com/davidtheclark/js-automation-examples#readme",
  "dependencies": {
    "autoprefixer-core": "^5.1.11",
    "babel": "^5.3.3",
    "browser-sync": "^2.7.1",
    "browserify": "^10.1.3",
    "clean-css": "^3.2.9",
    "del": "^1.1.1",
    "eslint": "^0.21.0",
    "grunt": "^0.4.5",
    "grunt-babel": "^5.0.0",
    "grunt-browser-sync": "^2.1.3",
    "grunt-browserify": "^3.8.0",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-cssmin": "^0.12.3",
    "grunt-contrib-uglify": "^0.9.1",
    "grunt-karma": "^0.10.1",
    "grunt-postcss": "^0.4.0",
    "grunt-sass": "^1.0.0",
    "gruntify-eslint": "^0.1.0",
    "gulp": "^3.8.11",
    "gulp-babel": "^5.1.0",
    "gulp-eslint": "^0.12.0",
    "gulp-minify-css": "^1.1.1",
    "gulp-postcss": "^5.1.6",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.0.1",
    "gulp-uglify": "^1.2.0",
    "karma": "^0.12.31",
    "load-grunt-tasks": "^3.1.0",
    "node-sass": "^3.1.0",
    "postcss": "^4.1.10",
    "rimraf": "^2.3.3",
    "uglify-js": "^2.4.21"
  }
}
