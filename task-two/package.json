{
  "name": "task-two",
  "version": "0.1.0",
  "description": "Simple JS compilation",
  "main": "index.js",
  "author": "David Clark",
  "license": "MIT",
  "dependencies": {
    "babelify": "^6.1.1",
    "browser-sync": "^2.7.1",
    "browserify": "^10.1.3",
    "del": "^1.1.1",
    "eslint": "^0.21.0",
    "grunt": "^0.4.5",
    "grunt-browser-sync": "^2.1.3",
    "grunt-browserify": "^3.8.0",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-uglify": "^0.9.1",
    "grunt-contrib-watch": "^0.6.1",
    "gruntify-eslint": "^0.1.0",
    "gulp": "^3.8.11",
    "gulp-eslint": "^0.12.0",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^1.2.0",
    "load-grunt-tasks": "^3.1.0",
    "parallelshell": "^1.1.1",
    "rimraf": "^2.3.3",
    "uglify-js": "^2.4.21",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watch": "^0.16.0"
  },
  "scripts": {
    "lint": "eslint src",
    "browserify": "browserify src/js/index.js -t babelify -o dist/js/bundled.js",
    "compress": "uglifyjs dist/js/bundled.js -o dist/js/bundled-uglified.js",
    "clean": "rimraf dist/js/*.*",
    "prejs:io": "npm run lint && npm run clean",
    "js:io": "npm run browserify && npm run compress",
    "prejs:stream": "npm run lint && npm run clean",
    "js:stream": "browserify src/js/index.js -t babelify | uglifyjs > dist/js/bundled-uglified.js",
    "bs": "browser-sync start --files \"dist/**/*\" --server",
    "watch": "watch \"npm run js:stream\" src/js",
    "dev": "parallelshell \"npm run bs\" \"npm run watch\""
  }
}
